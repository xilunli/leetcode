class Solution {
public:
    int trailingZeroes(int n) {
        int ans = 0;
        if( n <= 0) return ans;
        while(n>0) {
            n /= 5;
            ans += n;
        }
    return ans;  
    }
};

public class Solution {
    public int trailingZeroes(int n) {
        int ans = 0;
        if( n <= 0) return ans;
        while(n>0) {
            n /= 5;
            ans += n;
        }
    return ans;        
    }
}
