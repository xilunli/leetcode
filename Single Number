class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int ans = 0;
        for(int i=0; i<nums.size(); i++) {
            ans ^= nums[i];
        }
        return ans;
    }
};





public class Solution {
    public int singleNumber(int[] nums) {
        int ans = 0;
        for(int i=0; i<nums.length; i++) {
            ans ^= nums[i];
        }
        return ans;
    }
}
